{
  "name": "ticket_depo_rest",
  "version": "1.0.0",
  "description": "TicketDepo to create a RESTFUL API",
  "scripts": {
    "make-model": "npx sequelize-cli model:generate --name ticket --attributes first_name:string,last_name:string",
    "db-migrate": "npx sequelize-cli db:migrate",
    "migrate-create": "npx sequelize-cli migration:create --name",
    "db-reset": "npx sequelize-cli db:migrate:undo:all",
    "db-undo": "npx sequelize-cli db:migrate:undo",

    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./src/index",
    "db:create": "sequelize-cli db:create"
  },
  "author": "Diksha Bhatia",
  "license": "ISC",
  "dependencies": {
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.16.1",
    "uuid4": "^2.0.2",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1"
  }
}
